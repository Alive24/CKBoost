# CKBoost Protocol Configuration
# These values are required for the protocol to function properly

# Protocol Type Script Configuration
# If not set, the protocol management page will offer to deploy a new protocol cell
NEXT_PUBLIC_PROTOCOL_TYPE_CODE_HASH=
NEXT_PUBLIC_PROTOCOL_TYPE_HASH_TYPE=type
NEXT_PUBLIC_PROTOCOL_TYPE_ARGS=0x

# Network Configuration
NEXT_PUBLIC_CKB_NETWORK=testnet
NEXT_PUBLIC_CKB_RPC_URL=https://testnet.ckb.dev

# Optional: Custom node configuration
# NEXT_PUBLIC_CKB_INDEXER_URL=
